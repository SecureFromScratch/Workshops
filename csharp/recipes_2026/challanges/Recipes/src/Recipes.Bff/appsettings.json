{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore.Antiforgery": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "apiRoute": {
        "ClusterId": "apiCluster",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      }
    },
    "uploadsRoute": {
      "ClusterId": "apiCluster",
      "Match": {
        "Path": "/uploads/{**catch-all}"
      },
      "Order": 1
    },
    "Clusters": {
      "apiCluster": {
        "Destinations": {
          "api": {
            "Address": "http://localhost:7000/"
          }
        }
      }
    }
  }
}