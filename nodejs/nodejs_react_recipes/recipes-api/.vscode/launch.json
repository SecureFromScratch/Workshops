    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeArgs": [
                "--experimental-json-modules"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "program": "${workspaceFolder}/startup.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Erase DB",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeArgs": [
                "--experimental-json-modules"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "program": "${workspaceFolder}/storage/eraseAllContent.js"
        }
    ]
}