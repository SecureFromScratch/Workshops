{
  "name": "node-api",
  "version": "0.1.0",
  "type": "module",
  "main": "src/server.js",
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "scripts": {
    "seed": "prisma db seed",
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "db:up": "docker compose up -d db",
    "db:down": "docker compose down",
    "db:logs": "docker compose logs -f db",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev -n init",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "file-type": "^21.0.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "prisma": "^6.16.2"
  }
}
