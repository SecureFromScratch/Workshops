    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "BusyBee",
            "request": "launch",
            "mainClass": "com.securefromscratch.busybee.Application",
            "vmArgs": "-D\"java.util.logging.config.file=${workspaceFolder}/logging.properties\"",
            "projectName": "busybee",
            "env": {
                "JAVA_TOOL_OPTIONS": "-D\"java.util.logging.config.file=${workspaceFolder}/logging.properties\""
            }
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        }
    ]
}
