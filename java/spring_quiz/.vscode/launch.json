    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Application",
            "request": "launch",
            "mainClass": "com.securefromscratch.quiz.Application",
            "projectName": "spring_quiz",
            "vmArgs": "-Djava.util.logging.config.file=${workspaceFolder}/logging.properties",
            "env": {
                "JAVA_TOOL_OPTIONS": "-Djava.util.logging.config.file=${workspaceFolder}/logging.properties"
            }
        }
    ]
}